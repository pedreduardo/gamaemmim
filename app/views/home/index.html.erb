<!-- Header -->
<header id="header">
  <div class="intro">
    <div class="overlay">
      <div class="container">
        <div class="row">
          <div class="col-md-8 intro-text">
            <h1>Saiba como trabalhar e uma Startup!?</h1>
            <p>A oportunidade do emprego dos seus sonhos diante dos seus olhos.</p>
          </div>
          <div class="col-md-4 intro-text form">
            <%= form_with(model: Lead.new, remote: true) do |form| %>
                <%= form.text_field :name, id: :lead_name, placeholder: "Nome" %>
                <%= form.text_field :email, id: :lead_email, placeholder: "Email" %>
                <%= form.submit "Enviar" %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
<!-- Footer Section -->
<div id="footer">
  <div class="container text-center">
    <div class="col-md-8 col-md-offset-2">
      <div class="social">
        <ul>
          <li><a href="#"><i class="fa fa-facebook"></i></a></li>
          <li><a href="#"><i class="fa fa-twitter"></i></a></li>
          <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
        </ul>
      </div>
      <p>&copy; 2017. Designed by <a href="#" rel="nofollow">Gama Em Mim</a></p>
    </div>
  </div>
